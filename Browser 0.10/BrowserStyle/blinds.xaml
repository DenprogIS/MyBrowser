<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BrowserStyle"
             xmlns:ee="http://schemas.microsoft.com/expression/2010/effects" xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="BrowserStyle.blinds"
             mc:Ignorable="d" 
             d:DesignHeight="300" Width="547">
    <Grid>
        <Rectangle x:Name="rect" Fill="#27c9e0" Stroke="Black"/>

        <ed:Arc x:Name="arc" ArcThickness="20" ArcThicknessUnit="Pixel" EndAngle="300" Fill="White" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None" Stroke="#FF38A6F1" StartAngle="0" Width="177" Height="177">
            <ed:Arc.Effect>
                <DropShadowEffect BlurRadius="17"/>
            </ed:Arc.Effect>
        </ed:Arc>
        <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center" Height="90" Width="90">
            <Image x:Name="image" Source="Безымянный-1.png"/>
        </WrapPanel>
    </Grid>
</UserControl>

<UserControl x:Class="BrowserStyle.VKFriendShow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BrowserStyle"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="270" Height="50">
    <Grid>
        <Rectangle Fill="#FF27C9E0" Height="50" Stroke="Black" VerticalAlignment="Top" RadiusX="25" RadiusY="25"/>
        <Label x:Name="label" Content="Label" Margin="10,10,21,10" FontFamily="Showcard Gothic" FontSize="18.667" Foreground="White"/>
    </Grid>
</UserControl>
